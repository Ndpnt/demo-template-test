{
  "name": "demo-template-test",
  "services": {
    "repository": "https://github.com/Ndpnt/demo-template-test-declarations.git",
    "declarationsPath": "./declarations"
  },
  "recorder": {
    "versions": {
      "storage": {
        "git": {
          "snapshotIdentiferTemplate": "https://github.com/Ndpnt/demo-template-test-snapshots/commit/%SNAPSHOT_ID",
          "repository": "git@github.com:Ndpnt/demo-template-test-versions.git",
          "path": "./data/versions"
        }
      }
    },
    "snapshots": {
      "storage": {
        "git": {
          "repository": "git@github.com:Ndpnt/demo-template-test-snapshots.git",
          "path": "./data/snapshots"
        }
      }
    }
  },
  "fetcher": {
    "language": "en"
  },
  "notifier": {
    "sendInBlue": {
      "updatesListId": 0,
      "updateTemplateId": 0
    }
  },
  "tracker": {
    "githubIssues": {
      "repository": "Ndpnt/demo-template-test-declarations"
    }
  },
  "dataset": {
    "title": "demo-template-test",
    "versionsRepositoryURL": "https://github.com/Ndpnt/demo-template-test-versions"
  }
}
